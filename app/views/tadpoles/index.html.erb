<h1>Tadpoles</h1>
<p><a href="/tadpoles/new" class="btn btn-primary">Create a new tadpole</a></p>
<% @tadpoles.each do |tadpole| %>
  <div class="card">
    <a href="/tadpoles/<%= tadpole.id %>">
      <img src="http://<%= tadpole.color %>-tadpole.jpg.to/" alt="Tadpole Picture" height="115" width="175">
    </a>
    <p><a href="/tadpoles/<%= tadpole.id %>"><%= tadpole.name %></a></p>
  </div>
<% end %>

<h1>Frogs</h1>
<p><a href="/frogs/new" class="btn btn-primary">Create a new frog</a></p>

<% @frogs.each do |frog| %>
  <div class="card">
    <a href="/frogs/<%= frog.id %>">
      <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/7/76/Lithobates_sylvaticus_%28Woodfrog%29.jpg/1200px-Lithobates_sylvaticus_%28Woodfrog%29.jpg" alt="Frog Picture" height="115" width="175">
    </a>
    <p><a href="/frogs/<%= frog.id %>"><%= frog.name %></a></p>
  </div>
<% end %>
